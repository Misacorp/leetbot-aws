{
  "name": "leetbot-aws",
  "version": "0.1.0",
  "bin": {
    "leetbot-aws": "bin/leetbot-aws.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint --ext .js,.ts .",
    "aws:bootstrap": "npx cdk bootstrap",
    "aws:unbootstrap": "aws cloudformation delete-stack --stack-name CDKToolkit",
    "aws:deploy": "npm run build && npx cdk deploy --outputs-file cdk-outputs.json",
    "aws:destroy": "npx cdk destroy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.2",
    "@types/prettier": "2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "aws-cdk": "2.99.1",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.427.0",
    "@types/aws-lambda": "^8.10.123",
    "aws-cdk-lib": "2.99.1",
    "constructs": "^10.2.70",
    "discord.js": "^14.13.0"
  }
}
